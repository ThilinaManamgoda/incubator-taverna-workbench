<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                      http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="ParallelizeConfigureMenuAction" class="org.apache.taverna.workbench.parallelize.ParallelizeConfigureMenuAction">
			<property name="editManager" ref="editManager" />
			<property name="selectionManager" ref="selectionManager" />
	</bean>

	<bean id="ParallelizeContextualViewFactory" class="org.apache.taverna.workbench.parallelize.ParallelizeContextualViewFactory">
			<property name="editManager" ref="editManager" />
			<property name="selectionManager" ref="selectionManager" />
	</bean>

</beans>
